<div class="list-group-item list-group-item-action list-group-item-primary">
    <em class="bi bi-stars"></em>
    <%= t('audit_logs.audit_log.log_created', date: I18n.l(audit_log.created_at.in_time_zone(Settings.timezone)), user: audit_log.subject_name) %>
    <%= _('at IP address') %>
    <%= render partial: 'application/ip_address', locals: { ip: audit_log.ip } %>
    <br>
    <%= _('User Agent') %>: <code><%= audit_log.user_agent %></code>
    <br>
    <%= _('Referrer') %>: <code><%= audit_log.referrer.empty? ? _('None') : audit_log.referrer %></code>
</div>