<div class="list-group-item list-group-item-action list-group-item-warning">
    <em class="bi bi-x-octagon"></em>
    <%= _('Failed view attempt (already expired) by') %>
    <%= render partial: 'application/ip_address', locals: { ip: audit_log.ip } %>
    <%= _('on') %>
    <%= I18n.l audit_log.created_at.in_time_zone(Settings.timezone) %>
    <br>
    <%= t("audit_logs.audit_log.user_agent") %> <code><%= audit_log.user_agent.blank? ? t("audit_logs.audit_log.none") : audit_log.user_agent %></code>
    <br>
    <%= t("audit_logs.audit_log.referrer") %> <code><%= audit_log.referrer.blank? ? t("audit_logs.audit_log.none") : audit_log.referrer %></code>
</div>
